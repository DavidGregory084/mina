namespace Mina/Examples/DeclareBool {
    data Bool {
        case True()
        case False()
    }

    let id: A => A -> A = x -> x

    let idFn[A](a: A): A = a

    let const: [A, B] => (A, B) -> A = (a, b) -> a

    let constFn[A, B](a: A, b: B): A = a

    let foo = id(1)

    let bar =  idFn(1)

    let baz = const(const("a", "b"), id(2))

    let quu = constFn(1, 2)

    let quux = a -> constFn(a, 1)
}