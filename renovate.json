{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["local>mina-lang/renovate-config"],
  "packageRules": [
    {
      "matchPackageNames": "@types/node",
      "allowedVersions": "<17"
    },
    {
      "matchPackageNames": "vscode",
      "allowedVersions": "<=1.67.0"
    }
    {
      "matchPackageNames": "@types/vscode",
      "allowedVersions": "<1.68"
    },
    {
      "matchManagers": "npm",
      "rangeStrategy": "bump"
    }
  ]
}
