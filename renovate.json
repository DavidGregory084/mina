{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["local>mina-lang/renovate-config"],
  "packageRules": [
    {
      "matchPackageNames": "gradle",
      "allowedVersions": "<8.14"
    },
    {
      "matchPackageNames": "node",
      "allowedVersions": "<19"
    },
    {
      "matchPackageNames": "@types/node",
      "allowedVersions": "<19"
    },
    {
      "matchDepNames": "vscode",
      "matchDepTypes": "engines",
      "allowedVersions": "<=1.82.0"
    },
    {
      "matchPackageNames": "@types/vscode",
      "allowedVersions": "<1.83"
    },
    {
      "matchManagers": "npm",
      "rangeStrategy": "bump"
    }
  ]
}
